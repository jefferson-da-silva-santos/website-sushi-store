{
  "name": "website-sushi-store",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "watch:sass": "sass src/assets/sass/main.scss src/assets/css/styles.css -w",
    "compile:sass": "sass src/assets/sass/main.scss src/assets/css/styles.comp.css",
    "prefix:css": "postcss src/assets/css/styles.comp.css --use autoprefixer --output src/assets/css/styles.prefix.css",
    "compress:css": "postcss src/assets/css/styles.prefix.css --use cssnano --output src/assets/css/styles.css",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "dependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "aos": "^2.3.4",
    "boxicons": "^2.1.4",
    "notyf": "^3.10.0",
    "npm-run-all": "^4.1.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "@types/aos": "^3.0.7",
    "autoprefixer": "^10.4.20",
    "cssnano": "^7.0.6",
    "postcss": "^8.4.47",
    "postcss-cli": "^11.0.0",
    "sass": "^1.80.4"
  }
}